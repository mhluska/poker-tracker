{
  "name": "poker-tracker-frontend",
  "version": "0.0.1",
  "description": "Extremely simple frontend for a poker tracker Google Sheet",
  "main": "index.ts",
  "scripts": {
    "start": "cd build/ && python -m SimpleHTTPServer 8000",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc --esModuleInterop true --lib dom,es2015 --outDir build src/*.ts",
    "postbuild": "./scripts/postbuild.sh",
    "postinstall": "./scripts/postinstall.sh",
    "deploy": "gh-pages --dotfiles -d build",
    "lint:fix": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhluska/poker-tracker-frontend.git"
  },
  "keywords": [
    "poker",
    "tracker",
    "frontend"
  ],
  "author": "Maros Hluska <mhluska@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mhluska/poker-tracker-frontend/issues"
  },
  "homepage": "https://github.com/mhluska/poker-tracker-frontend#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "gh-pages": "^3.2.3",
    "modern-normalize": "^1.1.0",
    "typescript": "^4.6.2"
  }
}
